{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/1D",
      "dest": "/api/1D.js"
    },
    {
      "src": "/api/backtest",
      "dest": "/api/backtest.js"
    },
    {
      "src": "/api/valid",
      "dest": "/api/valid.js"
    }
  ]
}
